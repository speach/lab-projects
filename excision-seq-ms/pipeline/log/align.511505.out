    strand=${strands[$sidx]}
    strandarg=${strandargs[$sidx]}

    bedgraph=$bgresults/$sample.strand.$strand.counts.bg

    bedtools genomecov -5 -bg \
        -g $CHROM_SIZES -ibam $bam \
        $strandarg \
        > $bedgraph

